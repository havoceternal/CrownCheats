-- Replace with your Vercel URL
local API_URL = "https://protection-api.vercel.app/api/validate"

if not script_key or script_key == "" then
    return
end

local hwid = game:GetService("RbxAnalyticsService"):GetClientId()

local success, result = pcall(function()
    return game:HttpGet(API_URL .. "?key=" .. script_key .. "&hwid=" .. hwid)
end)

if not success then
    return
end

if result:find("--") or result:find("404") or result:find("403") then
    return
end

loadstring(result)()
